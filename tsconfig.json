{
    "compilerOptions": {
      "allowSyntheticDefaultImports": true,
      "lib": ["dom", "es2017"],
      "noImplicitAny": true,
      "module": "es6",
      "target": "es5",
      "jsx": "react",
      "allowJs": true,
      "moduleResolution": "node",
      "baseUrl": ".",
      "paths": {
        "@adapters/*": ["./adapters/*"],
        "@domains/*": ["./domains/*"],
        "@frameworks/*": ["./frameworks*"],
      }
    },
    "exclude": ["node_modules"]
  }
  